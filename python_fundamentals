# 44 Input :        two sum
#    4
#    2 7 11 15
#    9
# Output : 
#     0 1
def solve(n, nums, target):
    d = {}
    for i in range(n):
        currnum = nums[i]
        pairnum = target - nums[i]
        if pairnum in d:
            return[d[pairnum],i]
        d[currnum] = i

# 45 Input:         Determine If string Halves are Alive
# book
# Output: 
# 1
def solve(s):
    n = len(s)
    s1 = s[:n//2]
    s2 = s[n//2:]
    vowel = set("aeiouAEIOU")
    v1 = v2 = 0
    for c in s1:
        if c in vowel:
            v1+=1
    for c in s2:
        if c in vowel:
            v2+=1
    if v1 == v2:
        return 1
    return 0

# 46 Input:             fibonacci number
#     3
# Output:
#     2
def solve(n):
    if n == 0:
        return 0
    elif n == 1:
        return 1
    else:
        return solve(n-1)+solve(n-2)
    
# 47 Input : 4          Factorials
# Output : 24
def solve(n):
    if n == 0:
        return 1
    ans = n*solve(n-1)
    return ans

# 48 Input:     power(x,n)
#     3
#     4
# Output:
#     81
def solve(x, n):
    return x**n

# 49 Input: 22      Two's power
# Output: 0
def solve(n):
    if n == 1:
        return 1
    if n%2 == 1 or n == 0:
        return 0
    return solve(n//2)

  









        
    












