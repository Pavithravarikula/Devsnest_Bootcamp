print(x)

# 21 Input:     Nested loop-pattern 1
#     5
# Output:
#     1
#     12
#     123
#     1234
#     12345
x = int(input())
for i in range(1,x+1):
    for j in range(1,i+1):
        print(j,end="")
    print("")

# 22 Input:     pattern-2
#     5
# Output:
#     12345
#     1234
#     123
#     12
#     1
x = int(input())
for i in range(x,0,-1):
    for j in range(1,i+1):
        print(j,end="")
    print("")
# 23 Input:     pattern-3
#     5
# Output:
#     1
#    12
#   123
#  1234
# 12345
x = int(input())
for i  in range(1,x+1):
    for p in range(x-i):
        print(" ",end="")
    for j in range(1,i+1):
        print(j,end="")
    print("")

# 24 Input:     pattern-4
#     5
# Output:
# 12345
#  1234
#   123
#    12
#     1
x = int(input())
for i in range(x,0,-1):
    for j in range(1,i+1):
        print(j,end="")
    print("")

