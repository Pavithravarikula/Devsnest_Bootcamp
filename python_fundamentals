# 38 Input:         second largest element
#     6
#     2 1 2 8 7 8
# Output:
#     7
def solve(n, arr):
 
  k=max(arr)
  for i in arr:
     if i==k:
      arr.remove(i)
  return max(arr)

# 39 Input:         Devsnest pattern
#     9
#     27
# Output:
#     ------------.|.------------
#     ---------.|..|..|.---------
#     ------.|..|..|..|..|.------
#     ---.|..|..|..|..|..|..|.---
#     ---------DEVSNEST!---------
#     ---.|..|..|..|..|..|..|.---
#     ------.|..|..|..|..|.------
#     ---------.|..|..|.---------
#     ------------.|.------------
def solve(M, N):
    # CODE HERE
    arr=[]
    for i in range(1,M,2):
        arr.append((i*".|.").center(N,"-"))
    arr.append(("DEVSNEST!").center(N,"-"))
    for j in range(M-2,0,-2):
        arr.append((j*".|.").center(N,"-"))
    return arr

# 40 Input:         Mario Kart
#     5 2
#     3 37
#     1 41
#     2 37
#     5 41
#     4 35
# Output:
#     2 3
def solve(mat):
  d={}
  for i in mat:
    for j in i[1:]:
      if j in d:
        d[j].append(i[0])
      else:
        d[j]=[i[0]]

  l=list(d.keys())
  l.sort()
  return sorted(d[l[1]])

#  41 Input:        keep multiplying found values by 2
#       N=5
#       nums = 5 3 6 1 12
#       original = 3
# Output: 24    
def solve(N, nums, original):
    if original in nums:
        original*=2
    else:
        return original
    return solve(N,nums,original)

# 42  Input:        Unique no.of occurances
# 2
# 1 2
# Output: 
# 0
def solve(N, arr):
 m={}
 for i in arr:
    if i in m.keys():
        m[i]+=1
    else:
        m[i]=1

 se = set()
 for i in m.values():
    if i  not in se:
        se.add(i)
    else:
        return 0
 return 1

