if num=="add":
    n1=int(input())
    n2=int(input())
    print(n1+n2)
elif num=="subtract":
    n1=int(input())
    n2=int(input())
    print(n1-n2)
elif num=="multiply":
    n1=int(input())
    n2=int(input())
    print(n1*n2)
elif num=="divide":
    n1=int(input())
    n2=int(input())
    if n2==0:
        print("cannot be divided by zero")
    else:
        print(n1/n2)

