#  29 Input:         Join
#     "   apple,  banana,  orange,   grapefruit, kiwi   "
# Output:
#     "apple banana orange grapefruit kiwi"
i = input().split()
j = ""
k = j.join(i)
print(k.replace(',',' '))

# 30 Input:         Find the target
#     2 7 8 5 0 1
#     5
# Output:
#     3
nums = input().split()
target = int(input())
arr=[]
for i in nums:
    arr.append(int(i))
if target in arr:
    print(arr.index(target))
else:
    print('-1')

# 31 Input:     List -Append
#     3 4 -1 0 -5 -9 1 1 -2
# Output:
#     [3, 4, 1, 1]
num = input().split()
arr=[]
res=[]
for i in num:
    arr.append(int(i))
for j in arr:
    if j>0:
        res.append(j)
print(res)






        
    












